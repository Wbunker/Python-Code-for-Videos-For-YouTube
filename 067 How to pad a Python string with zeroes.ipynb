   "source": [
    "# Zfill is the magic spell to use for this\n",
    "\n",
    "### stands for zero fill (kind of gives it away)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "74a367a1",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "0045\n"
     ]
    }
   ],
   "source": [
    "episode = '45'\n",
    "\n",
    "\n",
    "print(str(episode).zfill(4))"
   ]
  },
  {
