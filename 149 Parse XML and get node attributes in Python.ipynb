    "import xml.etree.ElementTree as ET\n",
    "# <foo>\n",
    "#    <bar>\n",
    "#       <type foobar=\"1\"/>\n",
    "#       <type foobar=\"2\"/>\n",
    "#    </bar>\n",
    "# </foo>\n",
    "parse = ET.parse('data/foo.xml')\n",
    "root = parse.getroot()\n",
    "\n",
    "for tag in root.findall('bar/type'):\n",
    "    value = tag.get('foobar')\n",
    "    print(value)"
