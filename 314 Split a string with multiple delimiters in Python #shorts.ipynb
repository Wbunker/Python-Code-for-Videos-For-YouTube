    "import re\n",
    "s = \"Will; Bunker, 10|20\"\n",
    "\n",
    "# [] any of these characters\n",
    "# \\s whitespace * zero or more\n",
    "p = r\"[;,|]\\s*\"\n",
    "\n",
    "l1 = re.split(p, s)\n",
    "print(l1)\n"
